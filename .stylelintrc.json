{
  "extends": "stylelint-config-sass-guidelines",
  "rules": {
    "scss/at-extend-no-missing-placeholder": true,
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends"
      ]
    }],
    "block-no-empty": null,
    "unit-whitelist": ["px", "%", "s", "fr"]
  },
  "plugins": [
    "stylelint-scss"
  ]
}